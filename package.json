{
  "name": "create-react-app-typescript-rewired-styleguidist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma": "^0.7.1",
    "classnames": "^2.2.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "styleguide": "styleguidist server --scripts-version react-scripts-ts",
    "styleguide:build": "styleguidist build --scripts-version react-scripts-ts"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "node-sass": "^4.9.0",
    "react-app-rewired": "^1.5.2",
    "react-docgen-typescript": "^1.5.0",
    "react-scripts-ts": "2.16.0",
    "react-styleguidist": "^7.0.14",
    "sass-loader": "^7.0.1",
    "typescript": "^2.8.3"
  }
}
